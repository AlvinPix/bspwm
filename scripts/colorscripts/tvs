#!/bin/bash

f=3 b=4
for j in f b; do
  for i in {0..7}; do
    printf -v $j$i %b "\e[${!j}${i}m"
  done
done
bld=$'\e[1m'
rst=$'\e[0m'
inv=$'\e[7m'
w=$'\e[37m'
cat << EOF

$f1 ▀▄   ▄▀  $f2 ▀▄   ▄▀  $f3 ▀▄   ▄▀  $f4 ▀▄   ▄▀  $f5 ▀▄   ▄▀  $f6 ▀▄   ▄▀  
$f1 ▄▄█▄█▄▄  $f2 ▄▄█▄█▄▄  $f3 ▄▄█▄█▄▄  $f4 ▄▄█▄█▄▄  $f5 ▄▄█▄█▄▄  $f6 ▄▄█▄█▄▄  
$f1█$w██████$f1██ $f2█$w██████$f2██ $f3█$w██████$f3██ $f4█$w██████$f4██ $f5█$w██████$f5██ $f6█$w██████$f6██ 
$f1█$w██████$f1██ $f2█$w██████$f2██ $f3█$w██████$f3██ $f4█$w██████$f4██ $f5█$w██████$f5██ $f6█$w██████$f6██ 
$f1█$w██████$f1██ $f2█$w██████$f2██ $f3█$w██████$f3██ $f4█$w██████$f4██ $f5█$w██████$f5██ $f6█$w██████$f6██ 
$f1 ▀▀▀▀▀▀▀  $f2 ▀▀▀▀▀▀▀  $f3 ▀▀▀▀▀▀▀  $f4 ▀▀▀▀▀▀▀  $f5 ▀▀▀▀▀▀▀  $f6 ▀▀▀▀▀▀▀  $bld

$f1 ▀▄   ▄▀  $f2 ▀▄   ▄▀  $f3 ▀▄   ▄▀  $f4 ▀▄   ▄▀  $f5 ▀▄   ▄▀  $f6 ▀▄   ▄▀  
$f1 ▄▄█▄█▄▄  $f2 ▄▄█▄█▄▄  $f3 ▄▄█▄█▄▄  $f4 ▄▄█▄█▄▄  $f5 ▄▄█▄█▄▄  $f6 ▄▄█▄█▄▄  
$f1█$w██████$f1██ $f2█$w██████$f2██ $f3█$w██████$f3██ $f4█$w██████$f4██ $f5█$w██████$f5██ $f6█$w██████$f6██ 
$f1█$w██████$f1██ $f2█$w██████$f2██ $f3█$w██████$f3██ $f4█$w██████$f4██ $f5█$w██████$f5██ $f6█$w██████$f6██ 
$f1█$w██████$f1██ $f2█$w██████$f2██ $f3█$w██████$f3██ $f4█$w██████$f4██ $f5█$w██████$f5██ $f6█$w██████$f6██ 
$f1 ▀▀▀▀▀▀▀  $f2 ▀▀▀▀▀▀▀  $f3 ▀▀▀▀▀▀▀  $f4 ▀▀▀▀▀▀▀  $f5 ▀▀▀▀▀▀▀  $f6 ▀▀▀▀▀▀▀  $rst
EOF
